    <CameraManipulator>OrbitManipulator</CameraManipulator>   
    <SetUpViewInWindow>
      <x>50</x>
      <y>50</y>
      <width>1024</width>
      <height>768</height>
    </SetUpViewInWindow>
  </Viewer>  
