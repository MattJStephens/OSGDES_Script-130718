  <Group>
    <Name>RootGroup</Name>
    <Ref>
      <RefName>Scene</RefName>
      <EntityType>Group</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
   </Group>
  <!--★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★ Scene Group-->
      
