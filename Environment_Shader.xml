  <Image>
    <Name>Image1</Name>
    <FileName>Fieldstone.jpg</FileName>
  </Image>

  <Texture2D>
    <Name>Texture2D1</Name>
    <Ref>
      <RefName>Image1</RefName>
      <EntityType>Image</EntityType>
      <RefType>SetImage</RefType>
    </Ref>
  </Texture2D>

  <Image>
    <Name>Image2</Name>
