    <Ref>
      <RefName>Program2</RefName>
      <EntityType>Program</EntityType>
      <RefType>SetAttribute</RefType>
    </Ref>
    <Ref>
      <RefName>Uniform1</RefName>
      <EntityType>Uniform</EntityType>
      <RefType>AddUniform</RefType>
    </Ref>
    <Ref>
      <RefName>Uniform2</RefName>
      <EntityType>Uniform</EntityType>
      <RefType>AddUniform</RefType>
    </Ref>
    <Ref>
      <RefName>Uniform3</RefName>
      <EntityType>Uniform</EntityType>
      <RefType>AddUniform</RefType>
    </Ref>
    <Ref>
      <RefName>Uniform4</RefName>
