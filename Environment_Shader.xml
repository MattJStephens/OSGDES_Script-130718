    <SetMode>FAB_LINE</SetMode>
  </PolygonMode>  
  <!--☤☤☤ StateSet-->
  <StateSet>
    <Name>StateSet_Geode1</Name>
    <Ref>
      <RefName>PolygonMode1</RefName>
      <EntityType>PolygonMode</EntityType>
      <RefType>SetAttribute</RefType>
    </Ref>
    <Ref>
      <RefName>Program1</RefName>
      <EntityType>Program</EntityType>
      <RefType>SetAttribute</RefType>
    </Ref>
  </StateSet>  
