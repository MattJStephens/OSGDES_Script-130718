        <x>0</x>
        <y>0</y>
        <z>0</z>
      </Center>
      <x>1</x>
      <y>1</y>
      <z>2</z>
    </Box>
  </ShapeDrawable>
  <!--☤☤☤ Geode-->
  <Geode>
    <Name>Geode1</Name>
    <Ref>
      <RefName>ShapeDrawable1</RefName>
      <EntityType>ShapeDrawable</EntityType>
      <RefType>AddDrawable</RefType>
    </Ref>
    <Ref>
      <RefName>StateSet_Geode1</RefName>
      <EntityType>StateSet</EntityType>
      <RefType>SetStateSet</RefType>
    </Ref>
  </Geode>
  <!--☤☤☤ PAT-->
  <PositionAttitudeTransform>
    <Name>Geode1PAT</Name>
    <Position>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Position>
    <Ref>
      <RefName>Geode1</RefName>
      <EntityType>Geode</EntityType>
      <RefType>AddChild</RefType>
    </Ref> 
  </PositionAttitudeTransform>
  <!--✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪✪-Capsule1-->
  <!--☤☤☤ Shaders-->
  <Shader>
    <Name>VertShader2</Name>
    <Vert_file>gooch.vert</Vert_file>
  </Shader>
  <Shader>
    <Name>FragShader2</Name>
    <Frag_file>gooch.frag</Frag_file>
  </Shader>
  <!--☤☤☤ Uniform-->
  <Uniform>
    <Name>Uniform1</Name>
    <ID>LightPosition</ID>
    <Type>Vec3</Type>
