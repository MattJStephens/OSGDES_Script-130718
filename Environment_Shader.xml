  <!--☤☤☤ Program-->
  <Program>
    <Name>Program2</Name>
    <Ref>
      <RefName>VertShader2</RefName>
      <EntityType>Shader</EntityType>
      <RefType>AddShader</RefType>
    </Ref>
    <Ref>
      <RefName>FragShader2</RefName>
