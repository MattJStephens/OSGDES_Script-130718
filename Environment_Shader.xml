      <RefName>L1</RefName>
      <EntityType>Light</EntityType>
      <RefType>SetLight</RefType>
    </Ref>
    <Ref>
      <RefName>RootGroup</RefName>
      <EntityType>Group</EntityType>
      <RefType>SetStateSetModesOn</RefType>
    </Ref>
  </LightSource>
  <PositionAttitudeTransform>
    <Name>LS1PAT</Name>
    <Ref>
      <RefName>LS1</RefName>
      <EntityType>LightSource</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
    <Position>
      <x>0</x>
      <y>0</y>
      <z>5</z>
    </Position>
    <Scale>
      <x>0.1</x>
      <y>0.1</y>
      <z>0.1</z>
    </Scale>
  </PositionAttitudeTransform>
  <!--☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤ Line-->
  <Vec3Array>
    <Name>Vec3Array1</Name>
    <Vec3d>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Vec3d>
    <Vec3d>
      <x>0</x>
      <y>0</y>
      <z>50</z>
    </Vec3d>
    <Vec3d>
        <x>0</x>
        <y>0</y>
        <z>0</z>
      </Vec3d>
    <Vec3d>
        <x>0</x>
        <y>50</y>
        <z>0</z>
    </Vec3d>
    <Vec3d>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Vec3d>
    <Vec3d>
      <x>50</x>
      <y>0</y>
      <z>0</z>
    </Vec3d>
  </Vec3Array>
  <DrawArrays>
    <Name>DrawArrays1</Name>
    <PrimitiveSet>LINE_STRIP</PrimitiveSet>
    <SetFirst>0</SetFirst>
    <SetCount>6</SetCount>
  </DrawArrays>
  <Geometry>
    <Name>Geometry1</Name>
    <Ref>
      <RefName>DrawArrays1</RefName>
      <EntityType>DrawArrays</EntityType>
      <RefType>AddPrimitiveSet</RefType>
    </Ref>    
    <Ref>
      <RefName>Vec3Array1</RefName>
      <EntityType>Vec3Array</EntityType>
      <RefType>SetVertexArray</RefType>
    </Ref>   
  </Geometry>
  <Geode>
    <Name>GeodeLine1</Name>   
    <Ref>
      <RefName>Geometry1</RefName>
      <EntityType>Geometry</EntityType>
      <RefType>AddDrawable</RefType>
    </Ref>
    <Ref>
      <RefName>White</RefName>
      <EntityType>Material</EntityType>
      <RefType>SetAttribute</RefType>
    </Ref>
  </Geode>
  <PositionAttitudeTransform>
    <Name>GeodeLine1PAT</Name>
    <Position>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Position>
    <Ref>
      <RefName>GeodeLine1</RefName>
      <EntityType>Geode</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
  </PositionAttitudeTransform>
  
  <!--☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤ WIREFRAME GEOMETRY-->
  <!--☤☤☤ Shaders-->
  <Shader>
    <Name>VertShader1</Name>
    <Vert_file>RGBCube.vert</Vert_file>
  </Shader>
  <Shader>
    <Name>FragShader1</Name>
    <Frag_file>RGBCube.frag</Frag_file>
  </Shader>
  <!--☤☤☤ Program-->
  <Program>
    <Name>Program1</Name>
    <Ref>
      <RefName>VertShader1</RefName>
      <EntityType>Shader</EntityType>
      <RefType>AddShader</RefType>
    </Ref>
    <Ref>
      <RefName>FragShader1</RefName>
      <EntityType>Shader</EntityType>
      <RefType>AddShader</RefType>
    </Ref>
  </Program>
