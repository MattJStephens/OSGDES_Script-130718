<?xml version="1.0"?>
<!-- mjs OpenSceneGraph Extensible Markup Language Process Description Script (OSG XML PDS)-->
<!--✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠-->
<Event>
  <Name>Sequencer1</Name>
  <Type>Sequencer</Type>
  <Subordination>Simultaneous</Subordination>
  <Event>
    <Name>SaveEventTree</Name>
    <FileName>EventTree1.xml</FileName>
    <Type>SaveEventTree</Type>
    <Recurrance>Singular</Recurrance>
  </Event>
  <!--★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★ AlterText -->
  <Event>
    <Name>AlterText1</Name>
    <Type>AlterText</Type>
    <AlterString>ElapsedTime</AlterString>
    <RefName>T1</RefName>
  </Event>
  <Event>
    <Name>AlterText2</Name>
    <Type>AlterText</Type>
    <AlterString>ElapsedSimulationTime</AlterString>
    <RefName>T2</RefName>
  </Event>
  <Event>
    <Name>DE_Logistics1</Name>
    <Type>DE_Logistics</Type>
    <Variable>
      <Identifier>Group</Identifier>
      <Value>Scene</Value>
      <DataType>String</DataType>
    </Variable>
    
    <Variable>
      <Identifier>TimeType</Identifier>
      <Value>Real</Value>
      <DataType>String</DataType>
    </Variable>
    <Variable>
      <Identifier>Logistics_producer</Identifier>
      <Value>Logistics_producer1</Value>
      <DataType>String</DataType>
    </Variable>
    <Variable>
      <Identifier>Logistics_component</Identifier>
      <Value>Logistics_component1</Value>
      <DataType>String</DataType>
    </Variable>
    <Variable>
      <Identifier>Logistics_demand</Identifier>
      <Value>Logistics_demand1</Value>
      <DataType>String</DataType>
    </Variable>
    <Variable>
      <Identifier>Logistics_demand</Identifier>
      <Value>Logistics_demand2</Value>
      <DataType>String</DataType>
    </Variable>
    <Variable>
      <Identifier>Logistics_demand</Identifier>
      <Value>Logistics_demand3</Value>
      <DataType>String</DataType>
    </Variable>  
    <Variable>
      <Identifier>Speed</Identifier>
      <Value>30</Value>
      <DataType>Float</DataType>
    </Variable>
    <Variable>
      <Identifier>StackingDistance</Identifier>
      <Value>1.5</Value>
      <DataType>Float</DataType>
    </Variable>
    
  </Event>
  <Event>
    <Name>Sequencer2</Name>
    <Type>Sequencer</Type>
    <Subordination>PeriodicSimultaneous</Subordination>
    <Period>1</Period>    
    <Event>
      <Name>UpdateSimulationTime1</Name>
      <Type>UpdateSimulationTime</Type>
      <SimulationTimeIncrement>10</SimulationTimeIncrement>
    </Event>
    <!--use producer instead of source 
    logistics will locate demand-->
    <!--<Event>
      <Name>Source1</Name>
      <Type>Source</Type>
      <SourceType>Logistics_node</SourceType>
      <Logistics_node_name>Logistics_node1</Logistics_node_name>     
    </Event>-->
    
  </Event>
</Event>