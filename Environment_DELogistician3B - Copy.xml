  <DEDemand>
    <Name>DEDemand1</Name>
    <Type>Periodic</Type>
    <ComponentRef>
      <RefName>DEComponent1</RefName>
    </ComponentRef>
    <NodeRef>
      <RefName>DENode2</RefName>
    </NodeRef>
    <SourceNodeRef>
      <RefName>DENode1</RefName>
    </SourceNodeRef>   
    <Period>0.9</Period>
    <OperativeState>transit_destroy_at_destination</OperativeState>
  </DEDemand>
  <DEDemand>
    <Name>DEDemand2</Name>
    <Type>Periodic</Type>
    <ComponentRef>
      <RefName>DEComponent1</RefName>
