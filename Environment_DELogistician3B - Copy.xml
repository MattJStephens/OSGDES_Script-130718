  <Geode>
    <Name>HUD_Geode</Name>
    <Ref>
      <RefName>T1</RefName>
      <EntityType>Text</EntityType>
      <RefType>AddDrawable</RefType>
    </Ref>
    <Ref>
      <RefName>T2</RefName>
      <EntityType>Text</EntityType>
