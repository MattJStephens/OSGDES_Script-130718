    <GeodeRef>
      <RefName>Cylinder1</RefName>
      <EntityType>Geode</EntityType>   
    </GeodeRef>
  </DEComponent>
  <!--☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤ DEProducer-->
