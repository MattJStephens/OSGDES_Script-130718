    <PeriodTotal>0.2</PeriodTotal>
    <ComponentRef>
      <RefName>DEComponent1</RefName>
    </ComponentRef>
    <NodeRef>
      <RefName>DENode1</RefName>
    </NodeRef>
  </DEProducer>
