<?xml version="1.0"?>
<!-- mjs OpenSceneGraph Extensible Markup Language Environment Description Script (OSG XML EDS)-->
<!--✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈✈-->
<Environment>
  <!--♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛♛ media folders -->
  <FilePath>I:\textures\</FilePath>
  <FilePath>I:\3ds\</FilePath>
  <FilePath>I:\OSG_DES\3ds\</FilePath>
  <FilePath>I:\OSG_DES\OSG\</FilePath>
  <FilePath>I:\OSG_DES\</FilePath>
  <FilePath>I:\OSG_DES\shaders\</FilePath>
  <FilePath>I:\media\architecture\</FilePath>
  <FilePath>C:\WINDOWS\Fonts\</FilePath>
  <!-- ☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆ set active indicies-->
  <ActiveViewerIndex>0</ActiveViewerIndex>
  <ActiveGroupIndex>0</ActiveGroupIndex>
  <!-- ☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆☆ Root Group--> 
  <Group>
    <Name>RootGroup</Name>
    <Ref>
      <RefName>Scene</RefName>
      <EntityType>Group</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
   </Group>
  <!--★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★ Scene Group-->
      
  <Group>
    <Name>Scene</Name>    
    <Ref>
      <RefName>ClearNode1</RefName>
      <EntityType>ClearNode</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
    <Ref>
      <RefName>LS1PAT</RefName>
      <EntityType>PAT</EntityType>
      <RefType>AddChild</RefType>
    </Ref>  
    <Ref>
      <RefName>GeodeLine1PAT</RefName>
      <EntityType>PAT</EntityType>
      <RefType>AddChild</RefType>
    </Ref>

    <Ref>
      <RefName>PhysXPlane1</RefName>
      <EntityType>PhysXPlane</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
    <Ref>
      <RefName>PhysXFluid1</RefName>
      <EntityType>PhysXFluid</EntityType>
      <RefType>AddChild</RefType>
    </Ref>  
  
    <Ref>
      <RefName>HUDProjectionMatrix</RefName>
      <EntityType>Projection</EntityType>
      <RefType>AddChild</RefType>
    </Ref>   
  </Group>
  <!--★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★ Viewer-->
  <CustomCamera>
    <!--<VelocityCoefficient>5</VelocityCoefficient>-->
    <VelocityCoefficient>5</VelocityCoefficient>
    <!--<MouseCoefficient>0.001</MouseCoefficient>-->
    <MouseCoefficient>0.001</MouseCoefficient>
    <UpAxis>X</UpAxis>
  </CustomCamera>
  <KeyBoardEventHandler>enable</KeyBoardEventHandler>      
  <Viewer>
    <Name>Viewer1</Name>
    <!--<CameraManipulator>TrackballManipulator</CameraManipulator>-->
    <!--<CameraManipulator>FlightManipulator</CameraManipulator>-->
    <!--<CameraManipulator>DriveManipulator</CameraManipulator>-->
    <!--<CameraManipulator>OrbitManipulator</CameraManipulator>-->
    <!--<CameraManipulator>FirstPersonManipulator</CameraManipulator>-->
    <!--<CameraManipulator>OrbitManipulator</CameraManipulator>-->   
    <Ref>
      <RefName>KeyBoardEventHandler</RefName>
      <EntityType>KeyBoardEventHandler</EntityType>
      <RefType>AddEventHandler</RefType>
    </Ref>   
    <SetUpViewInWindow>
      <x>50</x>
      <y>50</y>
      <width>1024</width>
      <height>768</height>
    </SetUpViewInWindow>
  </Viewer>  
  <!-- ⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙⊙ Background color-->
  <ClearNode>
    <Name>ClearNode1</Name>
    <ClearColor>
      <x>0.1</x>
      <y>0.1</y>
      <z>0.1</z>
      <w>1</w>
    </ClearColor>
  </ClearNode>    
  <!--♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋♋ Materials -->
  <Material>
    <Name>White</Name>
    <Diffuse>
      <x>1</x>
      <y>1</y>
      <z>1</z>
      <w>1</w>
    </Diffuse>
    <Specular>
      <x>0.1</x>
      <y>0.1</y>
      <z>0.1</z>
      <w>1</w>
    </Specular>
    <Ambient>
      <x>0.1</x>
      <y>0.1</y>
      <z>0.1</z>
      <w>1</w>
    </Ambient>
    <Emission>
      <x>0.3</x>
      <y>0.3</y>
      <z>0.3</z>
      <w>1</w>
    </Emission>
    <Shininess>25</Shininess>
  </Material>
  <!--☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀☀ Lighting-->
  <Light>
    <Name>L1</Name>
    <Position>
      <x>0</x>
      <y>0</y>
      <z>0</z>
      <w>1</w>
    </Position>
    <Diffuse>
      <x>1</x>
      <y>1</y>
      <z>1</z>
      <w>1</w>
    </Diffuse>
    <Direction>
      <x>0.5</x>
      <y>0.5</y>
      <z>-0.5</z>
    </Direction>
    <Specular>
      <x>0.5</x>
      <y>0.5</y>
      <z>0.5</z>
      <w>1</w>
    </Specular>
    <Ambient>
      <x>0.5</x>
      <y>0.5</y>
      <z>0.5</z>
      <w>1</w>
    </Ambient>
    <SetConstantAttenuation>1</SetConstantAttenuation>
  </Light>
  <LightSource>
    <Name>LS1</Name>
    <SetLocalStateSetModes>ON</SetLocalStateSetModes>
    <Ref>
      <RefName>L1</RefName>
      <EntityType>Light</EntityType>
      <RefType>SetLight</RefType>
    </Ref>
    <Ref>
      <RefName>RootGroup</RefName>
      <EntityType>Group</EntityType>
      <RefType>SetStateSetModesOn</RefType>
    </Ref>
  </LightSource>
  <PositionAttitudeTransform>
    <Name>LS1PAT</Name>
    <Ref>
      <RefName>LS1</RefName>
      <EntityType>LightSource</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
    <Position>
      <x>0</x>
      <y>0</y>
      <z>5</z>
    </Position>
    <Scale>
      <x>0.1</x>
      <y>0.1</y>
      <z>0.1</z>
    </Scale>
  </PositionAttitudeTransform>
  <!--☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢☢    Image-->
  <Image>
    <Name>ScreenGrab</Name>
    <!--<FileName>ScreenGrab.jpg</FileName>-->
  </Image>
  <!--☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤☤ Line-->
  <Vec3Array>
    <Name>Vec3Array1</Name>
    <Vec3d>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Vec3d>
    <Vec3d>
      <x>0</x>
      <y>0</y>
      <z>50</z>
    </Vec3d>
    <Vec3d>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Vec3d>
    <Vec3d>
      <x>0</x>
      <y>50</y>
      <z>0</z>
    </Vec3d>
    <Vec3d>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Vec3d>
    <Vec3d>
      <x>50</x>
      <y>0</y>
      <z>0</z>
    </Vec3d>
  </Vec3Array>
  <DrawArrays>
    <Name>DrawArrays1</Name>
    <PrimitiveSet>LINE_STRIP</PrimitiveSet>
    <SetFirst>0</SetFirst>
    <SetCount>6</SetCount>
  </DrawArrays>
  <Geometry>
    <Name>Geometry1</Name>
    <Ref>
      <RefName>DrawArrays1</RefName>
      <EntityType>DrawArrays</EntityType>
      <RefType>AddPrimitiveSet</RefType>
    </Ref>
    <Ref>
      <RefName>Vec3Array1</RefName>
      <EntityType>Vec3Array</EntityType>
      <RefType>SetVertexArray</RefType>
    </Ref>
  </Geometry>
  <Geode>
    <Name>GeodeLine1</Name>
    <Ref>
      <RefName>Geometry1</RefName>
      <EntityType>Geometry</EntityType>
      <RefType>AddDrawable</RefType>
    </Ref>
    <Ref>
      <RefName>White</RefName>
      <EntityType>Material</EntityType>
      <RefType>SetAttribute</RefType>
    </Ref>
  </Geode>
  <PositionAttitudeTransform>
    <Name>GeodeLine1PAT</Name>
    <Position>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Position>
    <Ref>
      <RefName>GeodeLine1</RefName>
      <EntityType>Geode</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
  </PositionAttitudeTransform>
  <!--✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵   PhysX-->
  <PhysXPlane>
    <Name>PhysXPlane1</Name>
    <Origin>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Origin>
    <PlaneSize>100</PlaneSize>
  </PhysXPlane>
  <PhysXFluid>
    <Name>PhysXFluid1</Name>
    <Origin>
      <x>0</x>
      <y>0</y>
      <z>100</z>
    </Origin>
    <Gravity>
      <x>0</x>
      <y>0</y>
      <z>-9.8</z>
    </Gravity>
    <ParticleSize>100</ParticleSize>
    <GenerationRadius>4</GenerationRadius>
  </PhysXFluid>   
  
  
  <!--✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵✵   Geode containing text-->
  <Geode>
    <Name>HUD_Geode</Name>
    <Ref>
      <RefName>T1</RefName>
      <EntityType>Text</EntityType>
      <RefType>AddDrawable</RefType>
    </Ref>
    <Ref>
      <RefName>T2</RefName>
      <EntityType>Text</EntityType>
      <RefType>AddDrawable</RefType>
    </Ref>
    <Ref>
      <RefName>T2</RefName>
      <EntityType>Text</EntityType>
      <RefType>AddDrawable</RefType>
    </Ref>
  </Geode>
  <!--☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸☸    Text-->
  <Text>
    <Name>T1</Name>
    <CharacterSize>18</CharacterSize>
    <Font>impact.ttf</Font>
    <InitText>PhysX - Fluid, Matt J. Stephens 2012</InitText>
    <AxisAlignment>SCREEN</AxisAlignment>
    <Position>
      <x>10</x>
      <y>10</y>
      <z>-1.5</z>
    </Position>
    <Color>
      <x>199</x>
      <y>77</y>
      <z>150</z>
      <w>1</w>
    </Color>
  </Text>
 
  <!--☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠☠    Text ProjectionMatrix-->
  <Projection>
    <Name>HUDProjectionMatrix</Name>
    <Ortho2D>
      <Left>0</Left>
      <Right>1024</Right>
      <Bottom>0</Bottom>
      <Top>768</Top>
    </Ortho2D>
    <Ref>
      <RefName>HUDModelViewMatrix</RefName>
      <EntityType>MatrixTransform</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
  </Projection>
  <!--✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦✦  Text ViewMatrix Transfom -->
  <MatrixTransform>
    <Name>HUDModelViewMatrix</Name>
    <Identity></Identity>
    <ReferenceFrame>ABSOLUTE_RF</ReferenceFrame>
    <Ref>
      <RefName>HUD_Geode</RefName>
      <EntityType>Geode</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
  </MatrixTransform>
  
</Environment>