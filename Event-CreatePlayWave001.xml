<?xml version="1.0"?>
<!--mjs-->
<Event>
  <Name>BaseSequencer</Name>
  <Type>Sequencer</Type>
  <Subordination>Simulataneous</Subordination>
  <Event>
    <Name>FirstCreateWave</Name>
    <Type>CreateWave</Type>
    <Inception>Spontaneous</Inception>
    <Recurrance>Singular</Recurrance>
    <Variable>
      <Identifier>FileName</Identifier>
      <Value>wave-random001.wav</Value>
    </Variable>
    <Variable>
      <Identifier>Channels</Identifier>
      <Value>1</Value>
    </Variable>
    <Variable>
      <Identifier>SampleRate</Identifier>
      <Value>44100</Value>
    </Variable>
    <Variable>
      <Identifier>BitsPerSample</Identifier>
      <Value>16</Value>
    </Variable>
    <Variable>
      <Identifier>NumSamples</Identifier>
      <Value>44100</Value>
    </Variable>
    <Variable>
      <Identifier>WaveForm</Identifier>
      <Value>sin</Value>
    </Variable>
  
    <Variable>
      <Identifier>Frequency</Identifier>
      <Value>440</Value>
    </Variable>
    <Variable>
      <Identifier>Dynamics</Identifier>
      <Value>AttackVibeDecay</Value>      
    </Variable>
    <Variable>
      <Identifier>SustainVibeFrequency</Identifier>
      <Value>5.0</Value>
    </Variable>
    <Variable>
      <Identifier>Attack</Identifier>
      <Value>0.3</Value>
    </Variable>
    <Variable>
      <Identifier>Sustain</Identifier>
      <Value>0.6</Value>
    </Variable>
    <Variable>
      <Identifier>Decay</Identifier>
      <Value>1.0</Value>
    </Variable>  
  </Event>
  <Event>
    <Name>FirstPlayWave</Name>
    <Type>PlayWav</Type>
    <Inception>Spontaneous</Inception>
    <Recurrance>Singular</Recurrance>
    <Variable>
      <Identifier>File</Identifier>
      <Value>wave-random001.wav</Value>
    </Variable>
    <Response_xml>SubEvent-CreateWave001.xml</Response_xml>
    <Response_xml>SubEvent-PlayWave001.xml</Response_xml>
  </Event>
</Event>