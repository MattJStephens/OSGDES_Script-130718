<?xml version="1.0"?>
<!-- mjs OpenSceneGraph Extensible Markup Language Process Description Script (OSG XML PDS)-->
<!--✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠✠-->
<Event>
  <Name>Sequencer1</Name>
  <Type>Sequencer</Type>
  <Subordination>Simultaneous</Subordination>
  <!--★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★★ AlterText -->
  <Event>
    <Name>AlterText1</Name>
    <Type>AlterText</Type>
    <AlterString>ElapsedSimulationTime</AlterString>
    <RefName>T1</RefName>
  </Event>

  <Event>
    <Name>Emitter</Name>
    <Type>Emitter</Type>
    <Origin>
      <x>0</x>
      <y>0</y>
      <z>10</z>
    </Origin>
    <Velocity>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </Velocity>
    <Variable>
      <Identifier>UpdateMode</Identifier>
      <Value>EXPR</Value>
    </Variable>
    <Variable>
      <Identifier>OriginRandomness</Identifier>
      <Value>20</Value>
    </Variable>
    <Variable>
      <Identifier>Mass</Identifier>
      <Value>100</Value>
    </Variable>
    <Variable>
      <Identifier>EXPR_X_ref</Identifier>
      <Value>EXPRx</Value>
    </Variable>
    <Variable>
      <Identifier>EXPR_Y_ref</Identifier>
      <Value>EXPRy</Value>
    </Variable>
    <Variable>
      <Identifier>EXPR_Z_ref</Identifier>
      <Value>EXPRz</Value>
    </Variable>
    <Variable>
      <Identifier>Period</Identifier>
      <Value>1</Value>
    </Variable>
    <Variable>
      <Identifier>Group</Identifier>
      <Value>Scene</Value>
    </Variable>
    <Variable>
      <Identifier>GeodeName</Identifier>
      <Value>Geode_Sphere1</Value>
    </Variable>
    <Variable>
      <Identifier>Vec3dArrayName</Identifier>
      <Value>Vec3Array1</Value>
    </Variable>
    <Variable>
      <Identifier>Type</Identifier>
      <Value>Geode</Value>
    </Variable>
  </Event>

  <!--BEGIN SEQUENCER 2-->
  <Event>
    <Name>Sequencer2</Name>
    <Type>Sequencer</Type>
    <Subordination>PeriodicSimultaneous</Subordination>
    <Period>0.2</Period>
    <Event>
      <Name>ScreenGrab</Name>
      <Type>ScreenGrab</Type>
      <Variable>
        <Identifier>Ref_image</Identifier>
        <Value>ScreenGrab</Value>
      </Variable>
      <Variable>
        <Identifier>FileName</Identifier>
        <Value>screen_grab</Value>
      </Variable>
      <Variable>
        <Identifier>FileExtension</Identifier>
        <Value>jpg</Value>
      </Variable>
      <Variable>
        <Identifier>Height</Identifier>
        <Value>300</Value>
      </Variable>
      <Variable>
        <Identifier>Width</Identifier>
        <Value>400</Value>
      </Variable>
      <Variable>
        <Identifier>BitColor</Identifier>
        <Value>24</Value>
      </Variable>
    </Event>
  </Event>
  <!--END SEQUENCER 2-->
</Event>