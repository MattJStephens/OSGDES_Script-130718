<?xml version="1.0"?>
<!--mjs-->

<Event>
  <Name>BaseSequencer</Name>
  <Type>Sequencer</Type>
  <Subordination>Simulataneous</Subordination>
  <Event>
    <Name>FirstCreateWave</Name>
    <Type>CreateWave</Type>
    <Inception>Spontaneous</Inception>
    <Recurrance>Singular</Recurrance>

    <Variable>
      <Identifier>FileName</Identifier>
      <Value>wave-randomform2.wav</Value>
    </Variable>
    <Variable>
      <Identifier>Channels</Identifier>
      <Value>1</Value>
    </Variable>
    <Variable>
      <Identifier>SampleRate</Identifier>
      <Value>22050</Value>
    </Variable>
    <Variable>
      <Identifier>BitsPerSample</Identifier>
      <Value>16</Value>
    </Variable>
    <Variable>
      <Identifier>MinRandNumSamples</Identifier>
      <Value>800000</Value>
    </Variable>
    <Variable>
      <Identifier>RandNumSamples</Identifier>
      <Value>800000</Value>
    </Variable>
    <Variable>
      <Identifier>RandomForm</Identifier>
      <Value>3</Value>
    </Variable>
    <Variable>
      <Identifier>Base_freq</Identifier>
      <Value>220</Value>
    </Variable>
    <Variable>
      <Identifier>Min_freq</Identifier>
      <Value>20</Value>
    </Variable>
    <Variable>
      <Identifier>Max_freq</Identifier>
      <Value>1500</Value>
    </Variable>
    <Variable>
      <Identifier>RandomChords2</Identifier>
      <Value>6</Value>
    </Variable>
    <Variable>
      <Identifier>RandomN</Identifier>
      <Value>17</Value>
    </Variable>
    <Variable>
      <Identifier>RandomStartFrequency</Identifier>
      <Value>220</Value>
    </Variable>
    <Variable>
      <Identifier>RandomEndFrequency</Identifier>
      <Value>220</Value>
    </Variable>
    <Variable>
      <Identifier>Dynamics</Identifier>
      <Value>Stephens</Value>
    </Variable>
    <Variable>
      <Identifier>ParamC</Identifier>
      <Value>0.5</Value>
    </Variable>
    
  </Event>
  <Event>
    <Name>FirstPlayWave</Name>
    <Type>PlayWav</Type>
    <Inception>Spontaneous</Inception>
    <Recurrance>Singular</Recurrance>
    <Variable>
      <Identifier>File</Identifier>
      <Value>wave-sweep-mix.wav</Value>
    </Variable>
    <Response_xml>SubEvent-CreateWave008.xml</Response_xml>
    <Response_xml>SubEvent-PlayWave008.xml</Response_xml>
  </Event>
</Event>