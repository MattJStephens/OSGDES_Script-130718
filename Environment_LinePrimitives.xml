      <EntityType>DrawArrays</EntityType>
      <RefType>AddPrimitiveSet</RefType>
    </Ref>    
    <Ref>
      <RefName>Vec3Array1</RefName>
      <EntityType>Vec3Array</EntityType>
      <RefType>SetVertexArray</RefType>
    </Ref>   
