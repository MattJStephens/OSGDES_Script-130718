      <EntityType>Light</EntityType>
      <RefType>SetLight</RefType>
    </Ref>
    <Ref>
      <RefName>RootGroup</RefName>
      <EntityType>Group</EntityType>
      <RefType>SetStateSetModesOn</RefType>
    </Ref>
  </LightSource>
  <PositionAttitudeTransform>
    <Name>LS1PAT</Name>
