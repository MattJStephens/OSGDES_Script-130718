      <RefType>AddDrawable</RefType>
    </Ref>
    <Ref>
      <RefName>Red</RefName>
      <EntityType>Material</EntityType>
      <RefType>SetAttribute</RefType>
    </Ref>
  </Geode>
  <PositionAttitudeTransform>
    <Name>GeodeLine1PAT</Name>
