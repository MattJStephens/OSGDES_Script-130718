<?xml version="1.0"?>
<!--mjs-->
<Environment>
  <ActiveGroupIndex>0</ActiveGroupIndex>
  <ActiveViewerIndex>0</ActiveViewerIndex>
  <Group>
    <Name>RootGroup</Name>
    <Ref>
      <RefName>Geode001</RefName>
      <EntityType>PAT</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
    <Ref>
      <RefName>PAT002</RefName>
      <EntityType>PAT</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
    <Ref>
      <RefName>HUDProjectionMatrix</RefName>
      <EntityType>Projection</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
  </Group>
  <Viewer>
    <Name>Viewer1</Name>
    <CameraManipulator>OrbitManipulator</CameraManipulator>
    <SetUpViewInWindow>
      <x>50</x>
      <y>50</y>
      <width>1024</width>
      <height>768</height>
    </SetUpViewInWindow>
  </Viewer>
  <Geode>
    <Name>Geode001</Name>
    <Sphere>
      <Center>
        <x>0</x>
        <y>0</y>
        <z>0</z>
      </Center>
      <Radius>1</Radius>
    </Sphere>
  </Geode>
  <PositionAttitudeTransform>
    <Name>Geode001</Name>
    <Ref>
      <RefName>Geode001</RefName>
      <EntityType>Geode</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
    <Position>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Position>
  </PositionAttitudeTransform>
  <Light>
    <Name>Light001</Name>
    <Position>
      <x>0</x>
      <y>0</y>
      <z>0</z>
      <w>1</w>
    </Position>
    <Diffuse>
      <x>1</x>
      <y>0</y>
      <z>0</z>
      <w>1</w>
    </Diffuse>
    <Specular>
      <x>0.5</x>
      <y>0.5</y>
      <z>0.5</z>
      <w>1</w>
    </Specular>
    <Ambient>
      <x>0.5</x>
      <y>0.5</y>
      <z>0.5</z>
      <w>1</w>
    </Ambient>
  </Light>
  <LightSource>
    <Name>LightSource001</Name>
    <SetLocalStateSetModes>ON</SetLocalStateSetModes>
    <Ref>
      <RefName>Light001</RefName>
      <EntityType>Light</EntityType>
      <RefType>SetLight</RefType>
    </Ref>
    <Ref>
      <RefName>RootGroup</RefName>
      <EntityType>Group</EntityType>
      <RefType>SetStateSetModesOn</RefType>
    </Ref>
  </LightSource>
  <Socket>
    <Name>Socket001</Name>
    <Type>Server</Type>
    <SocketAddressFamily>AF_INET</SocketAddressFamily>
    <SocketType>SOCK_DGRAM</SocketType>
    <SocketProtocol>0</SocketProtocol>
    <!--<NodeName>127.0.0.1</NodeName>-->
    <NodeName>INADDR_ANY</NodeName>
    <ServiceName>12345</ServiceName>
  </Socket>

  <!--text-->
  <Text>
    <Name>T1</Name>
    <CharacterSize>25</CharacterSize>
    <Font>impact.ttf</Font>
    <InitText>Server</InitText>
    <AxisAlignment>SCREEN</AxisAlignment>
    <Position>
      <x>10</x>
      <y>10</y>
      <z>-1.5</z>
    </Position>
    <Color>
      <x>199</x>
      <y>77</y>
      <z>200</z>
      <w>1</w>
    </Color>
  </Text>
  <Geode>
    <Name>HUD_Geode</Name>
    <Ref>
      <RefName>T1</RefName>
      <EntityType>Text</EntityType>
      <RefType>AddDrawable</RefType>
    </Ref>   
  </Geode> 
  <MatrixTransform>
    <Name>HUDModelViewMatrix</Name>
    <Identity></Identity>
    <ReferenceFrame>ABSOLUTE_RF</ReferenceFrame>
    <Ref>
      <RefName>HUD_Geode</RefName>
      <EntityType>Geode</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
  </MatrixTransform>
  <Projection>
    <Name>HUDProjectionMatrix</Name>
    <Ortho2D>
      <Left>0</Left>
      <Right>1024</Right>
      <Bottom>0</Bottom>
      <Top>768</Top>
    </Ortho2D>
    <Ref>
      <RefName>HUDModelViewMatrix</RefName>
      <EntityType>MatrixTransform</EntityType>
      <RefType>AddChild</RefType>
    </Ref>
  </Projection>  
</Environment>